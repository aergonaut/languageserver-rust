{
  "name": "languageserver-rust",
  "main": "./lib/languageserver-rust",
  "version": "0.3.0",
  "description": "Adds support for RLS to Atom",
  "homepage": "https://github.com/aergonaut/languageserver-rust",
  "author": {
    "name": "Chris Fung",
    "url": "https://twitter.com/aergonaut"
  },
  "repository": "https://github.com/aergonaut/languageserver-rust",
  "license": "MIT/Apache-2.0",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-languageclient": "^0.6.1"
  },
  "devDependencies": {
    "eslint": "^4.6.1"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "nuclide-datatip.provider": {
      "versions": {
        "0.0.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "nuclide-outline-view": {
      "versions": {
        "0.0.0": "provideOutlines"
      }
    },
    "nuclide-definition-provider": {
      "versions": {
        "0.0.0": "provideDefinitions"
      }
    },
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "provideHyperclick"
      }
    },
    "nuclide-find-references.provider": {
      "versions": {
        "0.0.0": "provideFindReferences"
      }
    }
  }
}
